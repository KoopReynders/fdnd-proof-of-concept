<div class="overzicht-ingeklokt switch-view" id="overzicht-ingeklokt">
	<% if (clockedInMwByIdArr.length > 0) { %>
        <% clockedInMwByIdArr.forEach((clockIn, index) => { %>
            <% if (clockIn.length == 1) { %>
                <div class="card">
                    <picture class="avatar-wrap">
                        <img class="avatar-img" src="<%- clockIn[0].avatar %>" alt="Avatar van <%- clockIn[0].name %>" width="40" height="40">
                    </picture>

                    <div class="card-content">
                        <h2 class="mw-name"><%- clockIn[0].name %></h2>
                        <p class="start-time"><%- clockIn[0].ClockInTime %> - <%- clockIn[0].TimePastClockIn %></p>
                    </div>
                </div>
            <% } else{ %>
                <details>
                    <summary>
                        <div class="card">
                            <picture class="avatar-wrap">
                                <img class="avatar-img" src="<%- clockIn[0].avatar %>" alt="Avatar van <%- clockIn[0].name %>" width="40" height="40">
                            </picture>

                            <div class="card-content">
                                <h2 class="mw-name"><%- clockIn[0].name %></h2>
                                <p class="start-time">Bekijk alle inklokmomenten</p>
                            </div>
                        </div>
                    </summary>

                    <% clockIn.forEach(mw => { %>
                        <div class="card">
                            <picture class="avatar-wrap">
                                <img class="avatar-img" src="<%- mw.avatar %>" alt="Avatar van <%- mw.name %>" width="40" height="40">
                            </picture>

                            <div class="card-content">
                                <h2 class="mw-name"><%- mw.ClockInTime %></h2>
                                <p class="start-time"><%- mw.TimePastClockIn %></p>
                            </div>
                        </div>
                    <% }) %>
                </details>
            <% } %>
        <% }) %>
	<% } else { %>
		<div class="empty-card">
			<picture class="svg-wrap">
				<img class="svg-img" src="/assets/employees.svg" alt="Employees icon" width="40" height="40">
			</picture>

			<div class="empty-card-content">
				<h2 class="empty-text">Er zijn geen medewerkers ingeklokt</h2>
			</div>
		</div>
	<% } %>

	<!-- Clock in button -->
	<a href="/inklokken" class="clock-btn clock-in">
		<img src="/assets/inklok.svg" alt="Clock in icon" class="clock-svg" width="25" height="25">
	</a>
</div>