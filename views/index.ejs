<h1>Book of Potions</h1>

<h2>All potions</h2>
<% potions.forEach(potion => { %>
  <h3><%= potion.name %></h3>
  <img src="<%- potion.asset %>" alt="<%= potion.name %>">
<% }) %> 

<h2>All ingredients</h2>
<% ingredients.forEach(ingredient => { %>
  <h3><%= ingredient.name %></h3>
  <img src="<%- ingredient.asset %>" alt="<%= ingredient.name %>">
<% }) %> 